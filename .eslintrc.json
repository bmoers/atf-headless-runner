{
    "env": {
        "browser": false,
        "node": true,
        "commonjs": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:node/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": "warn",
        "quotes": [
            "error", "single", {
                "avoidEscape": true

            }
        ]
    }
}
